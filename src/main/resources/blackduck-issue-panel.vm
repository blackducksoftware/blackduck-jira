$webResourceManager.requireResource("com.blackducksoftware.integration.hub-jira:blackduck-jira-web-panel-resources")

<script language="JavaScript" type="text/javascript">
    try {
        hideHubCustomFields();
        replaceProjectOwnerField();
    } catch (error) {
        
    }
</script>

#if ($bdsHubProject || $bdsHubProjectVersion || $bdsHubProjectVersionNickname)
    <div class="module">
        <div class="mod-header"><h5 class="toggle-title">$i18n.getText("blackduck.integration.jira.issue.custom.section.project.label")</h5></div>
        <ul class="property-list two-cols">
            #if ($bdsHubProject)
                #set( $side = "left" )
                	<li class="item">
                		<div class="hubWrap">
                			<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.project.label"):</strong>
                			<div id="hubProjectName" class="value"><a href="$bdsHubProjectVersionUrl" target="_blank">$bdsHubProject</a></div>
                		</div>
                	</li>
                	#set( $side = "right" )
            #end
            #if ($bdsHubProjectVersion)
                	#if ($side == "left")
                	   <li class="item">
                	   #set( $side = "right" )
                	#else
                	   <li class="item-right fiftyPercent">
                	   #set( $side = "left" )
                	#end
            		<div class="hubWrap">
            			<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.project.version.label"):</strong>
            			<div id="hubProjectVersionName" class="value">$bdsHubProjectVersion</div>
            		</div>
                	</li>
            #end
            #if ($bdsHubProjectVersionNickname)
                	#if ($side == "left")
                	   <li class="item">
                	   #set( $side = "right" )
                	#else
                	   <li class="item-right fiftyPercent">
                	   #set( $side = "left" )
                	#end
                		<div class="hubWrap">
                			<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.project.version.nickname.label"):</strong>
                			<div id="hubProjectVersionNickname" class="value">$bdsHubProjectVersionNickname</div>
                		</div>
                	</li>
            #end
        </ul>
    </div>
    <div class="fieldSectionSeparator"/>
#end          
            
#if ($bdsHubComponent || $bdsHubComponentVersion || $bdsHubComponentUsage || $bdsHubComponentOrigin || $bdsHubComponentOriginId || $bdsHubLicenses || $bdsHubProjectVersionLastUpdated)
    <div class="module">
        <div class="mod-header"><h5 class="toggle-title">$i18n.getText("blackduck.integration.jira.issue.custom.section.component.label")</h5></div>
            <ul class="property-list two-cols">
            
            #set( $side = "left" )
            
            #if ($bdsHubComponent)
                	<li class="item">
                    	<div class="hubWrap">
                    		<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.component.label"):</strong>
                    		#if ($bdsHubComponentVersionUrl)
                    		    <div id="hubComponentName" class="value"><a href="$bdsHubComponentVersionUrl" target="_blank">$bdsHubComponent</a></div>
                    		#else
                            <div id="hubComponentName" class="value"><a href="$bdsHubComponentUrl" target="_blank">$bdsHubComponent</a></div>
                    	    #end
                    	</div>
                	</li>
                	#set( $side = "right" )
            #end
            #if ($bdsHubComponentVersion)
                	#if ($side == "left")
                    	<li class="item">
                    	#set( $side = "right" )
                	#else
                    	<li class="item-right fiftyPercent">
                    	#set( $side = "left" )
                	#end
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.component.version.label"):</strong>
                		<div id="hubComponentVersionName" class="value">$bdsHubComponentVersion</div>
                	</div>
                	</li>
            #end
            #if ($bdsHubComponentOrigin)
                	#if ($side == "left")
                    	<li class="item">
                    	#set( $side = "right" )
                	#else
                    	<li class="item-right fiftyPercent">
                    	#set( $side = "left" )
                	#end
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.component.origin.label"):</strong>
                		<div id="hubComponentOrigin" class="value">$bdsHubComponentOrigin</div>
                	</div>
                	</li>
            #end
            #if ($bdsHubComponentOriginId)
                	#if ($side == "left")
                    	<li class="item">
                    	#set( $side = "right" )
                	#else
                    	<li class="item-right fiftyPercent">
                    	#set( $side = "left" )
                	#end
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.component.origin.id.label"):</strong>
                		<div id="hubComponentOriginId" class="value">$bdsHubComponentOriginId</div>
                	</div>
                	</li>
            #end
            #if ($bdsHubComponentUsage)
                #if ($side == "left")
                    <li class="item">
                    #set( $side = "right" )
                #else
                    <li class="item-right fiftyPercent">
                    #set( $side = "left" )
                #end
                <div class="hubWrap">
                    <strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.component.usage.label"):</strong>
                    <div id="hubComponentUsage" class="value">$bdsHubComponentUsage</div>
                </div>
                </li>
            #end
            #if ($bdsHubLicenses)
                	#if ($side == "left")
                    	<li class="item">
                    	#set( $side = "right" )
                	#else
                    	<li class="item-right fiftyPercent">
                    	#set( $side = "left" )
                	#end
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.licenses.label"):</strong>
                		#if ($bdsHubLicenseUrl)
                		    <div id="hubPolicyLicenses" class="value"><a href="$bdsHubLicenseUrl" target="_blank">$bdsHubLicenses</a></div>
                		#else
                         <div id="hubPolicyLicenses" class="value">$bdsHubLicenses</div>
                		#end
                	</div>
                	</li>
            #end
            #if ($bdsHubProjectVersionLastUpdated)
                	#if ($side == "left")
                    	<li class="item">
                    	#set( $side = "right" )
                	#else
                    	<li class="item-right fiftyPercent">
                    	#set( $side = "left" )
                	#end
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.project.version.last.updated.label"):</strong>
                		<div id="hubProjectVersionLastUpdated" class="value">$bdsHubProjectVersionLastUpdated</div>
                	</div>
            	</li>
            #end
        </ul>
    </div>
    <div class="fieldSectionSeparator"/>
#end
            
#if ($bdsHubPolicyRule || $bdsHubPolicyRuleOverridable)
    <div class="module">
        <div class="mod-header"><h5 class="toggle-title">$i18n.getText("blackduck.integration.jira.issue.custom.section.policy.label")</h5></div>
        <ul class="property-list two-cols">
            
            #set( $side = "left" )
        
            #if ($bdsHubPolicyRule)
                #if ($side == "left")
                    <li class="item">
                    #set( $side = "right" )
                #else
                    <li class="item-right fiftyPercent">
                    #set( $side = "left" )
                #end
                <div class="hubWrap">
                    <strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.policy.rule.label"):</strong>
                    <div id="hubPolicyRuleName" class="value" title="$bdsHubPolicyRuleDescription"><a href="$bdsHubPolicyRuleUrl" target="_blank">$bdsHubPolicyRule</a></div>
                </div>
                	</li>
             #end
            	#if ($bdsHubPolicyRuleOverridable)
                    #if ($side == "left")
                        <li class="item">
                        #set( $side = "right" )
                    #else
                        <li class="item-right fiftyPercent">
                        #set( $side = "left" )
                    #end
                    <div class="hubWrap">
                        <strong class="name">$i18n.getText("blackduck.integration.jira.issue.custom.policy.rule.overridable.label"):</strong>
                        <div id="hubPolicyRuleOverridable" class="value">$bdsHubPolicyRuleOverridable</div>
                    </div>
                </li>
            #end
        </ul>
    </div>
#end