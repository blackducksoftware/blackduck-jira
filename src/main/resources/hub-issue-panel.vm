                	</div>
                	</li>
            #end
            #if ($bdsHubProjectVersionLastUpdated)
                	#if ($side == "left")
                    	<li class="item">
                    	#set( $side = "right" )
                	#else
                    	<li class="item-right fiftyPercent">
                    	#set( $side = "left" )
                	#end
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("hub.integration.jira.issue.custom.project.version.last.updated.label"):</strong>
                		<div id="hubProjectVersionLastUpdated" class="value">$bdsHubProjectVersionLastUpdated</div>
                	</div>
            	</li>
            #end
        </ul>
    </div>
    <div class="fieldSectionSeparator"/>
#end
            
#if ($bdsHubPolicyRule)
    <div class="module">
        <div class="mod-header"><h5 class="toggle-title">$i18n.getText("hub.integration.jira.issue.custom.section.policy.label")</h5></div>
        <ul class="property-list two-cols">
            	<li class="item">
                	<div class="hubWrap">
                		<strong class="name">$i18n.getText("hub.integration.jira.issue.custom.policy.rule.label"):</strong>
